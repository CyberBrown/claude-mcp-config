# MCP Manager Environment Variables
# Copy this file to .env and fill in your actual values

# Gemini API key for vibe-check server
GEMINI_API_KEY=your_gemini_api_key_here

# GitHub Personal Access Token for github server
GITHUB_PERSONAL_ACCESS_TOKEN=your_github_token_here

# Context7 API key for document library
CONTEXT7_API_KEY=your_context7_api_key_here

# GitMCP server URL (e.g., https://gitmcp.io/your-repo)
GITMCP_SERVER=your_gitmcp_server_url_here

# Cloudflare Access credentials for protected MCP servers (developer-guides, mnemo, nexus)
CF_ACCESS_CLIENT_ID=your_cf_access_client_id_here
CF_ACCESS_CLIENT_SECRET=your_cf_access_client_secret_here

# Add more keys as needed for custom servers
